<br/><br/>
<table class="tabelas" align="center" width="90%">
 <tr>
   <td colspan="2" align="left">
      <font  size=5 color="black"> <b> CONSULTA JOGO POR FAIXA ETÁRIA</b></font>
   </td>
  </tr>
  <tr>
    <td>
      <br/>
       <table>
        <tr>
          <td>
            <div class="botao">
            <table>
              <tr>
                <td>
                   <font  size="4" color="black"> Faixa Etária: <%= select :jogo,:faixaetaria, FAIXAETARIA.sort {|e0,e1| e0[0] <=> e1[0]}, :include_blank => '---Selecione---'%></font>
                 </td>
              </tr>
           </table>
           </div>
          </td>
         </tr>
        <tr>
          <td>
             <div class="botao">
              <table width="100%" align="left">
                <tr>
                   <td align="left" width="8%" ><%= button_to 'Voltar', homes_path, {:method => "get"} %></td>
                   <td align="left" ><input type="Button" Value="Imprimir" onclick="javascript:window.print()"></td>
                </tr>
             </table>
            </div>
          </td>
        </tr>
        <tr>
          <td>
          <%= observe_field 'jogo_faixaetaria',
               :update => "listagem",
               :url    => { :action => 'lista_faixaetaria' },
               :with   => 'jogo_faixaetaria' %>
            <div id="listagem"> </div>
         </td>
       </tr>
    </table>
  </td>
 </table>
